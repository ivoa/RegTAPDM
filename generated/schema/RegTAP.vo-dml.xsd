               <xsd:documentation>An identifier for a role in a data model that the data in this column represents.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.utype</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="std" type="xsd:int" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>If 1, the meaning and use of this column is reserved and defined by a standard model. If 0, it represents a database-specific column that effectively extends beyond the standard.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.std</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="datatype" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The type of the data contained in the column.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.datatype</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="extended_schema"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An identifier for the schema that the value given by the extended attribute is drawn from.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.extended_schema</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="extended_type"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A custom type for the values this column contains.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.extended_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="arraysize" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The shape of the array that constitutes the value, e.g., 4, *, 4*, 5x4, or 5x*, as specified by VOTable.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.arraysize</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="delim" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The ivoa:string that is used to delimit elements of an array value when arraysize is not '1'.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.delim</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="type_system"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The type system used, as a QName with a canonical prefix; this will ususally be one of vs:simpledatatype, vs:votabletype, and vs:taptype.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.type_system</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="flag" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Hash-separated keywords representing traits of the column. Recognized values include 'indexed', 'primary', and 'nullable'.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.flag</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="column_description"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A free-text description of the column's contents.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Column.column_description</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Interface">
      <xsd:annotation>
         <xsd:documentation>Information on access modes of a capability</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:Interface</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="cap_index"
                      type="xsd:nonNegativeInteger"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The index of the parent capability.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.cap_index</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="intf_index"
                      type="xsd:nonNegativeInteger"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An arbitrary identifier for the interfaces of a resource.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.intf_index</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="intf_type" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The type of the interface (vr:webbrowser, vs:paramhttp, etc).</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.intf_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="intf_role" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An identifier for the role the interface plays in the particular capability. If the value is equal to 'std' or begins with 'std:', then the interface refers to a standard interface defined by the standard referred to by the capability's standardID attribute.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.intf_role</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="std_version"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The version of a standard interface specification that this interface complies with. When the interface is provided in the context of a Capability element, then the standard being refered to is the one identified by the Capability's standardID element.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.std_version</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="query_type" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Hash-joined list of expected HTTP method (get or post) supported by the service.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.query_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="result_type"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The MIME type of a document returned in the HTTP response.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.result_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="wsdl_url" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The location of the WSDL that describes this Web Service. If NULL, the location can be assumed to be the accessURL with '?wsdl' appended.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.wsdl_url</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="url_use" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A flag indicating whether this should be interpreted as a base URL ('base'), a full URL ('full'), or a URL to a directory that will produce a listing of files ('dir').</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.url_use</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="access_url" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The URL at which the interface is found.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.access_url</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="mirror_url" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Secondary access URLs of this interface, separated by hash characters.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.mirror_url</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="authenticated_only"
                      type="xsd:int"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A flag for whether an interface is available for anonymous use (=0) or only authenticated clients are served (=1).</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Interface.authenticated_only</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="InterfaceParam">
      <xsd:annotation>
         <xsd:documentation>Input parameters for services</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:InterfaceParam</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="intf_index"
                      type="xsd:nonNegativeInteger"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The index of the interface this parameter belongs to.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.intf_index</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The name of the parameter.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="unit" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The unit associated with all values in the parameter.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.unit</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ucd" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A unified content descriptor that describes the scientific content of the parameter.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.ucd</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="utype" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An identifier for a role in a data model that the data in this parameter represents.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.utype</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="std" type="xsd:int" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>If 1, the meaning and use of this parameter is reserved and defined by a standard model. If 0, it represents a database-specific parameter that effectively extends beyond the standard.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.std</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="datatype" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The type of the data contained in the parameter.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.datatype</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="extended_schema"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An identifier for the schema that the value given by the extended attribute is drawn from.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.extended_schema</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="extended_type"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A custom type for the values this parameter contains.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.extended_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="arraysize" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The shape of the array that constitutes the value, e.g., 4, *, 4*, 5x4, or 5x*, as specified by VOTable.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.arraysize</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="delim" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The ivoa:string that is used to delimit elements of an array value when arraysize is not '1'.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.delim</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="param_use" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An indication of whether this parameter is required to be provided for the application or service to work properly (one of required, optional, ignored, or NULL).</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.param_use</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="param_description"
                      type="xsd:string"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A free-text description of the parameter's contents.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:InterfaceParam.param_description</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Relationship">
      <xsd:annotation>
         <xsd:documentation>Relationships between resources (like mirroring, derivation, serving a data collection)</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:Relationship</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="relationship_type"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The type of the relationship; these terms are drawn from a controlled vocabulary and are DataCite-compatible.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Relationship.relationship_type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="related_id" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The IVOA identifier for the resource referred to.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Relationship.related_id</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="related_name"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The name of resource that this resource is related to.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Relationship.related_name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Validation">
      <xsd:annotation>
         <xsd:documentation>Validation levels for resources and capabilities</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:Validation</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="validated_by"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The IVOA ID of the registry or organisation that assigned the validation level.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Validation.validated_by</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="val_level" type="xsd:int" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A numeric grade describing the quality of the resource description, when applicable, to be used to indicate the confidence an end-user can put in the resource as part of a VO application or research study.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Validation.val_level</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="cap_index"
                      type="xsd:nonNegativeInteger"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>If non-NULL, the validation only refers to the capability referenced here.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Validation.cap_index</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ResDate">
      <xsd:annotation>
         <xsd:documentation>A date associated with an event in the life cycle of the resource</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:ResDate</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="date_value"
                      type="xsd:dateTime"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A date associated with an event in the life cycle of the resource.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:ResDate.date_value</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="value_role" type="xsd:string" use="required">
         <xsd:annotation>
            <xsd:documentation>A ivoa:string indicating what the date refers to, e.g., created, availability, updated. This value is generally drawn from a controlled vocabulary.</xsd:documentation>
            <xsd:appinfo>
               <vodml-ref>RegTAP:ResDate.value_role</vodml-ref>
            </xsd:appinfo>
         </xsd:annotation>
      </xsd:attribute>
   </xsd:complexType>
   <xsd:complexType name="Detail">
      <xsd:annotation>
         <xsd:documentation>XPath-value pairs for members of resource or capability and their derivations that are less used and/or
               from VOResource extensions</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:Detail</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="cap_index"
                      type="xsd:nonNegativeInteger"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The index of the parent capability; if NULL the xpath-value pair describes a member of the entire resource.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Detail.cap_index</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="detail_xpath"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The xpath of the data item.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Detail.detail_xpath</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="detail_value"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>(Atomic) value of the member.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:Detail.detail_value</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AltIdentifier">
      <xsd:annotation>
         <xsd:documentation>An alternate identifier associated with this record</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:AltIdentifier</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence/>
      <xsd:attribute name="alt_identifier" type="xsd:string" use="required">
         <xsd:annotation>
            <xsd:documentation>An identifier for the resource or an entity related to the resource in URI form.</xsd:documentation>
            <xsd:appinfo>
               <vodml-ref>RegTAP:AltIdentifier.alt_identifier</vodml-ref>
            </xsd:appinfo>
         </xsd:annotation>
      </xsd:attribute>
   </xsd:complexType>
   <xsd:complexType name="STCSpatial">
      <xsd:annotation>
         <xsd:documentation>The spatial coverage of resources</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:STCSpatial</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="coverage" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A geometry representing the area a resource contains data for; this should be tight at least with a resolution of degrees.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCSpatial.coverage</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ref_system_name"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The reference frame coverage is written in. This is currently reserved and fixed to NULL. Clients should always add a constraint to NULL for this to avoid matching non-celestial resources later.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCSpatial.ref_system_name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="STCTemporal">
      <xsd:annotation>
         <xsd:documentation>The temporal coverage of resources, given as one or more intervals.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:STCTemporal</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="time_start" type="xsd:float" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Lower limit of a time interval covered by the resource in MJD.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCTemporal.time_start</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="time_end" type="xsd:float" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Upper limit of a time interval covered by the resource in MJD.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCTemporal.time_end</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="STCSpectral">
      <xsd:annotation>
         <xsd:documentation>The spectral coverage of resources, given as one or more intervals.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:STCSpectral</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="spectral_start"
                      type="xsd:float"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Lower limit (in Joules) of messenger energy interval covered by the resource (for the solar system barycenter).</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCSpectral.spectral_start</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="spectral_end"
                      type="xsd:float"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Upper limit (in Joules) of messenger energy interval covered by the resource (for the solar system barycenter).</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:STCSpectral.spectral_end</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="TAPTable">
      <xsd:annotation>
         <xsd:documentation>TAP-queriable tables.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>RegTAP:TAPTable</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="svcid" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>IVOA identifier of the TAP service making this table queriable.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:TAPTable.svcid</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="table_name" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The fully qualified name of the table. As per VODataService, this includes all catalog or schema prefixes needed to distinguish it in a query, and it comes with SQL delimiters where necessary.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:TAPTable.table_name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="table_title"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A descriptive, human-interpretable name for the table.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:TAPTable.table_title</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="table_description"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A free-text description of the table's contents.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:TAPTable.table_description</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="table_utype"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>An identifier for a concept in a data model that the data in this table as a whole represent.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>RegTAP:TAPTable.table_utype</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
